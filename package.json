{
  "name": "@dlenroc/appium-playwright-driver",
  "version": "0.0.2",
  "description": "Appium Playwright Driver",
  "author": "Corneliu Duplachi",
  "repository": "dlenroc/appium-playwright-driver",
  "keywords": [
    "playwright",
    "appium",
    "driver"
  ],
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "appium": {
    "mainClass": "Driver",
    "driverName": "playwright",
    "automationName": "Playwright",
    "platformNames": [
      "chromium",
      "firefox",
      "webkit"
    ]
  },
  "dependencies": {
    "@appium/base-driver": "^9.4.1",
    "appium-support": "^2.55.0",
    "asyncbox": "^2.9.4",
    "base-64": "^1.0.0",
    "jszip": "^3.10.1",
    "requireg": "^0.2.2"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.1",
    "playwright": "^1.39.0",
    "typescript": "^5.2.2"
  }
}
