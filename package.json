{
  "name": "@dlenroc/appium-playwright-driver",
  "version": "0.0.1",
  "description": "Appium Playwright Driver",
  "author": "Corneliu Duplachi",
  "repository": "dlenroc/appium-playwright-driver",
  "keywords": [
    "playwright",
    "appium",
    "driver"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=12"
  },
  "appium": {
    "mainClass": "Driver",
    "driverName": "playwright",
    "automationName": "Playwright",
    "platformNames": [
      "chromium",
      "firefox",
      "webkit"
    ]
  },
  "dependencies": {
    "appium-base-driver": "^8.0.0-beta.6",
    "appium-support": "^2.54.1",
    "asyncbox": "^2.8.0",
    "base-64": "^1.0.0",
    "jszip": "^3.7.1",
    "requireg": "^0.2.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/base-64": "^1.0.0",
    "@types/rimraf": "^3.0.2",
    "playwright": "^1.14.1",
    "rollup": "^2.56.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.2"
  }
}
